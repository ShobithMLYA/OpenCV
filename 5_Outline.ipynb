   "outputs": [],
   "source": [
    "image=cv2.imread(\"assets//Lamp.jpg\",0)\n",
    "cv2.imshow('outline', image)\n",
    "\n",
    "gaussian_blur = cv2.GaussianBlur(image.copy(), (17,17), 0)\n",
    "image_edge = cv2.Canny(gaussian_blur,100, 70)\n",
    "\n",
    "cv2.imshow('Outline',image_edge)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n"
   ]
